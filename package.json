    "@babel/plugin-transform-modules-commonjs": "^7.9.0",
    "@types/jest": "^25.2.1",
    "cheminfo-build": "^1.1.1",
    "cheminfo-tools": "1.23.3",
    "eslint": "^6.8.0",
    "eslint-config-cheminfo": "^3.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^25.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "rollup": "^2.7.3"
