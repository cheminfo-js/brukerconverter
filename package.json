{
  "name": "brukerconverter",
  "version": "2.1.1",
  "description": "Parse and convert Bruker raw data",
  "main": "lib/index.js",
  "module": "src/index.js",
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "scripts": {
    "build": "cheminfo-build --entry src/index.js --root BrukerConverter",
